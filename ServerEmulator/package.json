{
  "name": "fom-emulator",
  "version": "0.1.0",
  "description": "Face of Mankind Server Emulator",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "tsx src/tests/run-tests.ts",
    "test:login": "tsx src/tests/login_decode.test.ts",
    "test:unit": "tsx src/tests/run-tests.ts",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "server": "tsx src/index.ts",
    "raknet": "tsx src/raknet-test.ts",
    "fom": "tsx src/fom-server.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "fom",
    "emulator",
    "mmorpg",
    "preservation"
  ],
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "raknet-native": "^1.2.3"
  }
}
